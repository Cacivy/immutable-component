{
  "name": "immutable-component",
  "version": "1.0.2",
  "description": "immutable-component to make state immutable",
  "keywords": [
    "component",
    "immutable"
  ],
  "homepage": "https://github.com/ustccjw/immutable-component#readme",
  "bugs": {
    "url": "https://github.com/ustccjw/immutable-component/issues"
  },
  "license": "MIT",
